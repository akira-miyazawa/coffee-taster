{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/device",
      "@types/node",
      "@types/googlemaps"
    ]
  },
  "files": [
    "types/vue2-google-maps.d.ts",
  ],
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.vue",
    "./**/*.ts",
    "./**/*.tsx",
  ],
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
